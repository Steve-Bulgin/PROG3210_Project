<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.steve_bulgin.prog3210_finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="PROG3210 Newsreader"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".MainActivity" android:label="Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="io.github.steve_bulgin.prog3210_finalproject.SettingsActivity" android:label="Settings">
        </activity>
        <activity android:name="io.github.steve_bulgin.prog3210_finalproject.AboutActivity" android:label="About">
        </activity>
    </application>

</manifest>